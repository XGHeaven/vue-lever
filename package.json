{
  "name": "vue-lever",
  "version": "0.1.1",
  "description": "a plugin can help components to manage true/false state of data",
  "main": "dist/lever.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "rollup -c",
    "precommit": "lint-staged",
    "test": "BABEL_ENV=test ava"
  },
  "author": "XGheaven",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/XGHeaven/vue-lever.git"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.6.0",
    "babelrc-rollup": "^3.0.0",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.3",
    "prettier": "^1.5.3",
    "rollup": "^0.49.2",
    "rollup-plugin-babel": "^3.0.2",
    "vue": "^2.4.2"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write --no-semi --trailing-comma all --single-quote --print-width 120",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "dependencies": {}
}
